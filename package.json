{
  "name": "@adiynil/tourjs",
  "version": "0.0.3-beta.1",
  "description": "一个轻量级的页面引导库，帮助用户快速了解网页功能。",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/style.css",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true,force:true});\"",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "bash ./bin/publish.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "tour",
    "guide",
    "tutorial",
    "onboarding",
    "tooltip",
    "highlight",
    "intro",
    "introduction",
    "step-by-step",
    "feature-tour"
  ],
  "author": {
    "name": "adiynil",
    "email": "adiynil@gmail.com",
    "url": "https://github.com/adiynil"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adiynil/tourjs.git"
  },
  "bugs": {
    "url": "https://github.com/adiynil/tourjs/issues"
  },
  "homepage": "https://github.com/adiynil/tourjs#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/jump.js": "^1.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.2.1",
    "rollup": "^4.30.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-strip": "^1.2.2",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.11",
    "jump.js": "^1.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ],
  "publishConfig": {
    "access": "public"
  }
}
